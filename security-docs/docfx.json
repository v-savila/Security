{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/security/breadcrumb/toc.json",
      "extendBreadcrumb": true,
      "feedback_system": "None",
      "titleSuffix": "Security documentation"
    },
    "fileMetadata": {
      "titleSuffix": {
        "MSRC/**/**.md": "Security",
        "SDL/**/**.md": "Security Documentation"
      }
    },
    "template": [],
    "dest": "security-docs",
    "markdownEngineName": "markdig"
  }
}